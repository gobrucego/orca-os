{
  "name": "swift-agents-plugin",
  "version": "1.5.0",
  "description": "45 production-ready AI agents for Claude Code - Install specialized agents for Swift, testing, documentation, CI/CD, and more",
  "author": {
    "name": "Stijn Willems",
    "email": "stijn@dooz.be",
    "url": "https://github.com/doozMen"
  },
  "license": "MIT",
  "repository": "https://github.com/doozMen/swift-agents-plugin",
  "homepage": "https://github.com/doozMen/swift-agents-plugin",
  "keywords": [
    "agents",
    "swift",
    "ios",
    "testing",
    "documentation",
    "devops",
    "mcp",
    "cli",
    "development-tools"
  ],
  "components": {
    "agents": [
      "agent-writer",
      "architect",
      "azure-devops",
      "blog-content-writer",
      "claude-code-plugin-builder",
      "code-reviewer",
      "conference-specialist",
      "crashlytics-analyzer",
      "crashlytics-architecture-correlator",
      "crashlytics-cross-app-analyzer",
      "crashlytics-multiclone-analyzer",
      "deckset-presenter",
      "documentation-verifier",
      "firebase-ecosystem-analyzer",
      "generic-assistant",
      "ghost-specialist",
      "git-pr-specialist",
      "grdb-sqlite-specialist",
      "hummingbird-developer",
      "kmm-specialist",
      "release-manager",
      "secrets-manager",
      "spm-specialist",
      "swift-architect",
      "swift-build-runner",
      "swift-cli-installer",
      "swift-cli-tool-builder",
      "swift-developer",
      "swift-docc",
      "swift-format-specialist",
      "swift-grpc-temporal-developer",
      "swift-mcp-server-writer",
      "swift-modernizer",
      "swift-server",
      "swift-state-machine-specialist",
      "swift-testing-specialist",
      "swift-testing-xcode-specialist",
      "swiftui-specialist",
      "task-router",
      "technical-debt-eliminator",
      "technical-documentation-reviewer",
      "test-builder",
      "timestory-builder",
      "xcode-configuration-specialist",
      "xib-storyboard-specialist"
    ],
    "commands": [],
    "hooks": []
  },
  "requirements": {
    "claudeCode": ">=1.0.0",
    "os": ["darwin"],
    "swift": ">=6.1",
    "macos": ">=13.0"
  },
  "installation": {
    "type": "script",
    "preInstall": [
      "export PATH=\"$HOME/.swiftpm/bin:$PATH\""
    ],
    "command": "swift package experimental-install --product claude-agents",
    "postInstall": [
      "echo 'Installing all 45 agents globally...'",
      "claude-agents install --all --global",
      "echo 'Installation complete! Run claude-agents list to see all available agents.'"
    ]
  },
  "features": {
    "smartRouting": {
      "enabled": true,
      "description": "Task-router agent uses local LLM (edgeprompt MCP) for intelligent agent delegation"
    },
    "mcpIntegration": {
      "enabled": true,
      "servers": ["SwiftLens", "Context7", "SourceKit-LSP"],
      "description": "15 agents with built-in MCP server support for enhanced capabilities"
    },
    "costOptimization": {
      "enabled": true,
      "distribution": {
        "opus": 2,
        "sonnet": 30,
        "haiku": 13
      },
      "description": "Mixed model strategy optimizes cost vs performance (Opus for complex, Haiku for simple)"
    }
  },
  "categories": {
    "swift-development": [
      "swift-architect",
      "swift-developer",
      "swiftui-specialist",
      "swift-modernizer",
      "swift-testing-specialist",
      "swift-testing-xcode-specialist",
